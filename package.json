{
  "name": "electron-lark",
  "version": "1.0.5",
  "main": "src/main.js",
  "description": "An Electron application for Feishu(Lark)",
  "author": "Ericwyn <ericwyn@meetwhy.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ericwyn/electron-lark.git"
  },
  "keywords": [
    "Electron",
    "Feishu",
    "Lark",
    "飞书"
  ],
  "bugs": "https://github.com/Ericwyn/electron-lark/issues",
  "homepage": "https://github.com/Ericwyn/electron-lark",
  "scripts": {
    "start": "electron ."
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "appId": "cn.lark.app",
    "linux": {
      "category": "Office",
      "packageCategory": "Office",
      "synopsis": "An Electron application for Feishu(Lark)",
      "icon": "./build/icons",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "pacman",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "desktop": {
        "Encoding": "UTF-8"
      }
    }
  }
}
